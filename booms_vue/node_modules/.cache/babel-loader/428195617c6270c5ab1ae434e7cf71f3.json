x\" class=\"biao\" >\n        <h2 class=\"title\">测试平台</h2>\n        <el-form-item label=\"用户名\">\n          <el-input v-model=\"form.username\" placeholder=\"用户名\"></el-input>\n        </el-form-item>\n\n        <el-form-item label=\"用户密码\">\n          <el-input v-model=\"form.pwd\" placeholder=\"密码\" ></el-input>\n        </el-form-item>\n\n        <el-form-item>\n          <el-button type=\"primary\" @click=\"onSubmit\" class=\"btn_login\">登录</el-button>\n        </el-form-item>\n      </el-form>\n  </div>\n</template>\n\n<script>\nimport {Login} from \"@/api/login\"\nexport default {\n  data() {\n    return {\n      form: {\n          username:\"\",\n          pwd:''\n      },\n    };\n  },\n  methods: {\n    onSubmit(){\n        if(this.form.username.length>0 && this.form.pwd.length>0){\n            Login(this.form.username,this.form.pwd).then(res=>{\n                if(res.data.error_code==0){\n                    localStorage.setItem(\"user\",res.data.user)\n                    localStorage.setItem(\"token\",res.data.token)\n                    this.$router.push(\"/home\")\n                }else{\n                    this.$message.error(res.data.msg);\n                }\n            })\n        }else{\n            this.$message.error(res.data.msg);\n\n        }\n    },\n  },\n};\n</script>\n\n<style scoped>\n.biao{\n    width: 350px;\n    margin:200px auto;\n    background-color: #F5F5F5;\n    padding: 25px;\n    padding-top: 5px;\n    padding-left: 10px;\n    padding-bottom: 20px;\n    border-radius: 10px;\n}\n.btn_login{\n    width: 270px;\n}\n.rq{\n  position:absolute;\n  height: 100%;\n  width: 100%;\n  background-image: url(\"../../../public/log.jpg\");\n  background-repeat: no-repeat;\n  background-size: 100% 100%; \n}\n.title{\n  text-align: center;\n}\n</style>"],"sourceRoot":"src/views/Login"}]}