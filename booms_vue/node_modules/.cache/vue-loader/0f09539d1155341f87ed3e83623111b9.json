der style=\"width: 361px\">\n        <el-table-column prop=\"id\" label=\"ID\" width=\"180\"> </el-table-column>\n        <el-table-column prop=\"tagname\" label=\"标签名\" width=\"180\">\n        </el-table-column>\n      </el-table>\n    </div>\n  </div>\n</template>\n\n<script>\nimport { apitag, addtag } from \"../../api/apijk.js\";\nexport default {\n  created() {\n    this.getData();\n  },\n  data() {\n    return {\n      input: \"\",\n      input2: \"\",\n      tableData: [\n        { id: 1, name: \"zyh\" },\n        { id: 1, name: \"zyh\" },\n        { id: 1, name: \"zyh\" },\n      ],\n    };\n  },\n  methods: {\n    getData() {\n      apitag().then((res) => {\n        this.tableData = res.data.res;\n      });\n    },\n    check() {\n      apitag().then((res) => {\n        console.log(res.data);\n      });\n    },\n    addtag() {\n      addtag(this.input).then((res) => {\n        if (res.data.error_code == 0) {\n          this.$message({\n            message: \"添加成功\",\n            type: \"success\",\n          });\n          this.getData()\n        }else{\n            this.$message.error(res.data.msg)\n        }\n      });\n    },\n  },\n};\n</script>\n\n<style scoped>\n.el-input {\n  width: 150px;\n}\n.el-button {\n  margin-left: 10px;\n}\n.input2 {\n  margin-left: 20px;\n  width: 200px;\n}\n.tab {\n  margin-top: 20px;\n}\n</style>"]}]}